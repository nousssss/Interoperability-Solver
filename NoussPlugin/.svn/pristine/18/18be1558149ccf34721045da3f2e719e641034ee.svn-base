package org.processmining.plugins.workshop.spoilers;

import org.deckfour.xes.model.XLog;
import org.processmining.models.workshop.WorkshopModel;
import org.processmining.models.workshop.connections.AbstractWorkshopModelConnection;

/**
 * Connects an event log and a workshop model using the parameters used.
 * 
 * @author hverbeek
 *
 */
public class WorkshopMiningConnection extends AbstractWorkshopModelConnection<WorkshopMiningParameters> {

	/**
	 * Creates the connection between the log, model, and parameters.
	 * @param log The given event log.
	 * @param model The given workshop model.
	 * @param parameters The given conversion parameters.
	 */
	public WorkshopMiningConnection(XLog log, WorkshopModel model, WorkshopMiningParameters parameters) {
		super(log, model, parameters);
	}

}
